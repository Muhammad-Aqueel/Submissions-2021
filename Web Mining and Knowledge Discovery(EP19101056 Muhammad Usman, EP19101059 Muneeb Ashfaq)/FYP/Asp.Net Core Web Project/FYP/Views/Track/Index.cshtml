@model FYP.Models.VirtualList

<h4>Track</h4>
<hr />
<p class="text-danger">@ViewBag.msg</p>
@if (Model != null)
{
<div>
    <p>Booking #: @Html.DisplayFor(model => model.parcels.cid)</p>
    <p>Consignee #: @Html.DisplayFor(model => model.parcels.consignee)</p>
    <p>Consignee Address #: @Html.DisplayFor(model => model.parcels.consignee_address)</p>
    <p>Destination: @Html.DisplayFor(model => model.parcels.destination)</p>
    <p>COD: @Html.DisplayFor(model => model.parcels.cod)</p>
    <div class="row">
        <table class="table-bordered">
            <tr>
                <th>Status</th>
                <th>Date</th>
                <th>Time</th>
            </tr>
            @if (ViewBag.list != null)
            {
                @foreach (var item in ViewBag.list as IEnumerable<FYP.Models.VirtualList>)
                {
                    <tr>
                        <td>@item.status_lists.status</td>
                        <td>@item.booking_Statuss.date.ToString("yyyy-MM-dd")</td>
                        <td>@item.booking_Statuss.time</td>
                    </tr>
                }
            }
        </table>
    </div>
</div>
    <div>
        <br />
        <a asp-action="Index" asp-controller="Update" asp-route-id="@Model.parcels.cid">Update Status</a>
    </div>
}